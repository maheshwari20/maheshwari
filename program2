#include<stdio.h>
int main()
{
	int marks;
	printf("enter the marks: ");
	scanf("%d" ,&marks);
	
	if(marks>=85){
	
	printf("you got grade A");
     } 
	else if(marks >=70){
	
	printf("you got grade B");
    }
	else if (marks >=55){
	
	printf("you got grade C");
    }
	else if(marks >=45){
	
	printf("you got grade D");
    }
	else if(marks <40){
	
	printf("you got  Failed");
    }  
    
    
     
     
 }
